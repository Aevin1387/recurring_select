.rs_dialog_holder {padding-left:0px; background-color:#333; background-color:rgba(30,30,30,0.3); font-size:1em;
  .rs_dialog { position:absolute; left:10px; right:10px; margin:0px; display:block; z-index:51; 
	
	
		.rs_dialog_content {
			
			label.ui-select { display:inline-block;}
			div.ui-select {display:inline-block;} 
			
			.freq_option_section .day_holder {width:auto; padding-right:5px;
				.day_selector {white-space:nowrap; display:inline-block; padding:3px 0;}
			}
			
			
			.rs_calendar { width:185px;
				a {height:10px; width:15px; padding:8px 5px; }
			}
			
			.rs_summary span { font-size:90%; font-weight:normal; }
		}

	}
}
